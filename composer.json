{
  "name": "kennersoft/kennercore",
  "description": "KennerCORE. Open source application.",
  "homepage": "https://kennersoft.de",
  "license": "GPL-3.0-only",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "ext-pdo_mysql": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "composer/semver": "~1.4.0",
    "doctrine/dbal": "~2.4.2",
    "slim/slim": "~2.6.2",
    "mtdowling/cron-expression": "~1.0.3",
    "zendframework/zend-mail": "~2.10.0",
    "zendframework/zend-servicemanager": "~3.3.2",
    "monolog/monolog": "~1.20.0",
    "yzalis/identicon": "~1.1.0",
    "zordius/lightncandy": "0.95",
    "tecnickcom/tcpdf": "~6.2.17 ",
    "php-mime-mail-parser/php-mime-mail-parser": "~2.10.0",
    "zbateson/mail-mime-parser": "~0.4.8",
    "phpoffice/phpspreadsheet": "~1.8.0",
    "symfony/event-dispatcher": "~4.3.0",
    "symfony/workflow": "~4.3.0",
    "guzzlehttp/psr7": "^1.6"
  },
  "autoload": {
    "psr-0": {
      "": [
        "../../../data/migrations/",
        "../../../data/module-manager-events/"
      ],
      "Espo\\Custom": "../../../custom/"
    },
    "psr-4": {
      "Espo\\": "app/Espo/",
      "Treo\\": "app/Treo/"
    }
  },
  "extra": {
    "treoId": "Treo",
    "version": "3.25.x"
  }
}
